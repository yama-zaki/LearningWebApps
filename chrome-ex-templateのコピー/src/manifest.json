{
  "name": "オートアンサーPRテンプレート挿入",
  "description": "PR作成時にbranch名に対応したテンプレートを読み込みます",
  "version": "0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "32": "images/3P.png"
    }
  },
  "permissions": [
    "https://github.com/tecowl/AutoAnswer/compare/*",
    "storage",
    "activeTab",
    "declarativeContent"
  ],
  "icons": {
    "48": "images/3P.png"
  }
}
