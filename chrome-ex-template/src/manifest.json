{
  "name": "オートアンサーPRテンプレート挿入",
  "description": "PR作成時にbranch名に対応したテンプレートを読み込みます",
  "version": "0.1",
  "manifest_version": 2,
  "content_scripts": [{
    "matches": ["https://github.com/tecowl/AutoAnswer/compare/*"],
    "js": ["jquery-2.2.0.min.js", "ChEx.js", "content_script.js"],
    "persistent": false
  }],
  "permissions": [
    "https://github.com/tecowl/AutoAnswer/compare/*",
    "storage",
    "activeTab",
    "declarativeContent"
  ],
  "icons": {
    "48": "images/page_white_csharp.png"
  }
}
